{
	"name": "Open DocViewer Tabs",
	"version": "1.0",
	"description": "Opens DocViewer modules on Quercus (Canvas for UofT) in a new tab.",
	"manifest_version": 2,
	"content_scripts": [
		{
			"matches": ["*://q.utoronto.ca/*"],
			"js": ["content.js"]
		}
	],
	"browser_action": {
		"default_icon": "/assets/icon.png"
	},
	"background": {
		"scripts": ["background.js"]
	}
}