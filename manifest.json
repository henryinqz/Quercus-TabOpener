{
	"name": "Quercus Tab Opener",
	"version": "2.0.4",
	"description": "Opens Quercus modules in new tabs.",
	"manifest_version": 2,
	"permissions": ["contextMenus"],
	"browser_action": {
		"default_icon": {
			"16": "/assets/icon16.png",
			"48": "/assets/icon48.png",
			"128": "/assets/icon128.png"
		}
	},
	"icons": {
		"16": "/assets/icon16.png",
		"48": "/assets/icon48.png",
		"128": "/assets/icon128.png"
	},
	"background": {
		"scripts": ["background.js"]
	}
}