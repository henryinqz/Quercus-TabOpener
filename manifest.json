{
	"name": "Open DocViewer Tabs",
	"version": "2.0",
	"description": "Opens Quercus DocViewer modules in new tabs.",
	"manifest_version": 2,
	"permissions": ["contextMenus"],
	"browser_action": {
		"default_icon": "/assets/icon.png"
	},
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	}
}